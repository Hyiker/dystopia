{
  "name": "dystopia",
  "displayName": "Dystopia",
  "description": "VScode HDR image viewer",
  "version": "0.0.2",
  "icon": "images/dystopia.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "publisher": "CarbeneHu",
  "repository": {
    "url": "https://github.com/Hyiker/dystopia"
  },
  "categories": [
    "Other"
  ],
  "files": [
    "media/**"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "imagePreview.preview",
        "displayName": "Image preview",
        "selector": [
          {
            "filenamePattern": "*.exr"
          },
          {
            "filenamePattern": "*.hdr"
          },
          {
            "filenamePattern": "*.pfm"
          },
          {
            "filenamePattern": "*.uhdr"
          },
          {
            "filenamePattern": "*.png"
          },
          {
            "filenamePattern": "*.jpg"
          },
          {
            "filenamePattern": "*.jpeg"
          },
          {
            "filenamePattern": "*.tga"
          },
          {
            "filenamePattern": "*.bmp"
          },
          {
            "filenamePattern": "*.gif"
          },
          {
            "filenamePattern": "*.ppm"
          },
          {
            "filenamePattern": "*.pgm"
          },
          {
            "filenamePattern": "*.pnm"
          },
          {
            "filenamePattern": "*.psd"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
